page{
  background: #efeff4;
  padding-bottom: 140rpx;
}
.search{
  display: flex;
  height: 90rpx;
  background: #efeff4;
  position: relative;
}
.searchwrap{
  width: 100%;
  height: 60rpx;
  margin: 16rpx 200rpx 0 24rpx;
  border: 1rpx solid #cccccc;
  border-radius: 6rpx;
  background: #ffffff;
}
.searchinput{
  float: left;
  width: 400rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 10rpx 14rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}
.searchicon{
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin: 16rpx 0 0 16rpx;
  background: url(http://statics.myplas.com/myapp/img/searchIcon.png) no-repeat;
  background-size: 100% 100%;
}
.filter{
  position: absolute;
  text-align: center;
  color: #666666;
  font-size: 26rpx;
  width: 180rpx;
  line-height: 50rpx;
  right: 15rpx;
  top: 20rpx;
}
.downarrow{
  width: 14rpx;
  height: 14rpx;
  display: inline-block;
  margin: 0 0 0 10rpx;
  background: url(http://statics.myplas.com/myapp/img/downarrow.png) no-repeat;
  background-size: 100% 100%;
}
.payfans{
  display: flex;
  background: #ffffff;
  justify-content: space-around;
  height: 100rpx;
  border-top: 1rpx solid #d9d9d9;
  border-bottom: 1rpx solid #d9d9d9;
  font-size: 34rpx;
  margin: 0 0 20rpx 0
}
.fanswrap{
  margin: 10rpx 0 0 0;
}
.fansimg{
  width: 80rpx;
  height: 80rpx;
  float: left;
  border: 1rpx solid #ff9566;
  border-radius: 6rpx;
}
.fanstxt{ float: left; height: 80rpx; line-height: 80rpx; margin: 0 0 0 10rpx;}
#img1{ background: url(http://statics.myplas.com/myapp/img/fansimg.png) no-repeat center center #e0e0e0; background-size: 100% 100%;}
#img2{ background: url(http://statics.myplas.com/myapp/img/fansimg2.png) no-repeat center center #e0e0e0; background-size: 100% 100%;}
.nameul{
  padding: 0 20rpx;
  border-top: 1rpx solid #d9d9d9;
  border-bottom: 1rpx solid #d9d9d9;
  list-style: none;
  margin: 0;
  background: #FFFFFF;
  font-size: 28rpx;
}
.nameli{
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-bottom: 1rpx solid #D9D9D9;
}
.rightArrow{
    position: absolute;
    top:55rpx;
    right: 0;
    width: 44rpx;
    height: 44rpx;
    background: url(http://statics.myplas.com/myapp/img/classifyBack.png) no-repeat;
    background-size: 100% 100%;
}
.nameli:last-of-type{
  border-bottom: none;
}
.avatorwrap{
  width: 110rpx;
  height: 110rpx;
  float: left;
  position: relative;
}
.avator{
  width: 110rpx;
    height: 110rpx;
    border-radius: 12rpx;
    float: left;
    background: url("http://statics.myplas.com/upload/16/09/02/logos.jpg") no-repeat;
    background-size: 100% 100%;
    border: 2rpx solid #f88f5f;
    overflow: hidden;
}
.avatorimg{
  width: 110rpx;
  height: 110rpx;
}
.iconV {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    border: 1rpx solid #EEEEEE;
    border-radius: 50%;
    right: -14rpx;
    bottom: -10rpx;
}
.iconV.v1 {
    background: url(http://statics.myplas.com/myapp/img/v1.png) no-repeat;
    background-size: 100% 100%;
}
.iconV.v2 {
    background: url(http://statics.myplas.com/myapp/img/v2.png) no-repeat;
    background-size: 100% 100%;
}
.nameinfo{
  width: auto;
    margin: 0 10rpx 0 130rpx;
    position: relative;
    height: 112rpx;
}
.p{
  font-size: 26rpx;
  height: 36rpx;
    line-height: 36rpx;
    margin: 0;
    color: #000;
    width: 560rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
    -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
    -webkit-line-clamp: 1; /** 显示的行数 **/
    overflow: hidden;  /** 隐藏超出的内容 **/
}
.p.first{
    color: #333333;
}
.p.second{
    color: #63769d;
}
.p.third{
    color: #666666;
}
.icon{
  display: inline-block;
    width: 30rpx;
    height: 30rpx;
    position: relative;
    top: 4rpx;
    margin: 0 6rpx;
}
.icon.wxGs {
    background: url(http://statics.myplas.com/myapp/img/wxGs.png) no-repeat;
    background-size: 100% 100%;
}
.icon.wxName {
    background: url(http://statics.myplas.com/myapp/img/wxName.png) no-repeat;
    background-size: 100% 100%;
}

.more{ width: 100%; height: 60rpx; line-height: 60rpx; font-size: 32rpx; text-align: center; color: #666666;}

/* 底部导航 */
.footer{
    width: 100%;
    height: 120rpx;
    background: #ffffff;
    position: fixed;
    left: 0;
    bottom: 0;
    font-size: 26rpx;
    border-top: 1rpx solid #d9d9d9;
}
.footerli{
    width: 25%;
    height: 100rpx;
    float: left;
    text-align: center;
    font-size: 26rpx;
    color: #7a7e83;
    margin: 12rpx 0 0 0;
}
.footerwrap{
    width: 130rpx;
    height: 130rpx;
    margin: 0 auto;
}
.footerico{
    width: 60rpx;
    height: 60rpx;
    margin: 0 auto;
}
.releaseicon{
    width: 88rpx;
    height: 88rpx;
    margin: 6rpx auto 0 auto;
    background: url(http://statics.myplas.com/myapp/img/releaseicon.png) no-repeat;
    background-size: 100% 100%;
}
.footerOn{ color: #ff5000;}
.footerico.footer1{
    background: url(http://statics.myplas.com/myapp/img/zone2.png) no-repeat;
    background-size: 100% 100%;
}
.footerOn .footer1{
    background: url(http://statics.myplas.com/myapp/img/zone2On.png) no-repeat;
    background-size: 100% 100%;
}

.footerico.footer2{
    background: url(http://statics.myplas.com/myapp/img/zone.png) no-repeat;
    background-size: 100% 100%;
}
.footerOn .footer2{
    background: url(http://statics.myplas.com/myapp/img/zoneOn.png) no-repeat;
    background-size: 100% 100%;
}

.footerico.footer3{
    background: url(http://statics.myplas.com/myapp/img/zone5.png) no-repeat;
    background-size: 100% 100%;
}
.footerOn .footer3{
    background: url(http://statics.myplas.com/myapp/img/zone5On.png) no-repeat;
    background-size: 100% 100%;
}

.footerOn .footer4{
    background: url(http://statics.myplas.com/myapp/img/zone3On.png) no-repeat;
    background-size: 100% 100%;
}
.footerico.footer4{
    background: url(http://statics.myplas.com/myapp/img/zone3.png) no-repeat;
    background-size: 100% 100%;
}


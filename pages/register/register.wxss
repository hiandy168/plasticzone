/* register.wxss */
page{
  background: #efeff4;
}
.registerTitle {
    width: 100%;
    padding: 0 24rpx;
    margin: 20rpx 0 0 0;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    color: #333333;
}
.arrowLeft {
    width: 30rpx;
    height: 30rpx;
    display: inline-block;
    position: relative;
    top: 2rpx;
    margin: 0 20rpx 0 0;
    background: url(http://statics.myplas.com/myapp/img/arrow_left.png) no-repeat;
    background-size: 100% 100%;
}
.registerInput2 {
    width: 100%;
    padding: 0 24rpx;
    background: #FFFFFF;
    border-top: 1rpx solid #d6d7dc;
    border-bottom: 1rpx solid #d6d7dc;
}
.registerBox2 {
    width: 100%;
    height: 86rpx;
    font-size: 26rpx;
    position: relative;
    line-height: 86rpx;
    border-bottom: 1rpx solid #D9D9D9;
}

.registerBox2:last-of-type {
    border-bottom: none;
}

.registerBox2 strong {
    width: 160rpx;
    color: #333333;
    float: left;
    font-weight: normal;
}

.registerBox2 input[type=tel], .registerBox2 input[type=password], .registerBox2 input[type=text] {
    font-size: 26rpx;
    height: 86rpx;
    line-height: 86rpx;
    margin: 0;
    width: auto;
    padding: 0;
    background: none;
    border: none;
}